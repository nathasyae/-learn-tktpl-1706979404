<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/nav_graph"
app:startDestination="@id/masterFragment">

<fragment
    android:id="@+id/masterFragment"
    android:name="id.ac.ui.cs.mobileprogramming.nathasyaeliora.myapplication.MasterFragment"
    android:label="Master"
    tools:layout="@layout/fragment_master" >
    <action
        android:id="@+id/action_masterFragment_to_detailFragment"
        app:destination="@id/detailFragment"
        app:enterAnim="@anim/slide_in_right"
        app:exitAnim="@anim/slide_out_left"
        app:popEnterAnim="@anim/slide_in_left"
        app:popExitAnim="@anim/slide_out_right" />
</fragment>
<fragment
    android:id="@+id/detailFragment"
    android:name="id.ac.ui.cs.mobileprogramming.nathasyaeliora.myapplication.DetailFragment"
    android:label="Detail"
    tools:layout="@layout/fragment_detail" />
</navigation>